    </statement>
    <statement id="321.txt-1">
      <text>taproots seldom branched.</text>
      <structure id="o5846" name="taproot">
        <character name="architecture_or_shape" value="branched" modifier="seldom" />
      </structure>
    </statement>
    <statement id="321.txt-2">
      <text>Stems 1–5+, decumbent to ascending, purple, (rarely exceeding foliage before fruiting), sparsely villous or glabrate to ± densely villous distally.</text>
      <structure id="o5847" name="stem">
        <character char_type="range_value" name="count" from="1" to="5" upper_restricted="false" />
        <character name="habit" value="decumbent" />
        <character name="orientation" value="ascending" />
        <character name="coloration" value="purple" />
        <character name="pubescence" value="villous" modifier="sparsely" />
        <character name="pubescence" value="glabrate" modifier="sparsely" />
        <character name="pubescence" value="villous" modifier="± densely ; distally" />
      </structure>
    </statement>
    <statement id="321.txt-3">
      <text>Leaves 10+, horizontal to ± erect;</text>
      <structure id="o5848" name="leaf">
        <character name="count" char_type="range_value" from="10" upper_restricted="false" />
        <character name="orientation" value="horizontal" />
        <character name="orientation" value="erect" modifier="±" />
