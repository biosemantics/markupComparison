    <statement id="360.txt-9">
      <text>Involucres cylindric to campanulate, 13–15 × 3–5 mm.</text>
      <structure id="o6771" name="involucre">
        <character char_type="range_value" name="shape" from="cylindric" to="campanulate" />
        <character name="shape" value="cylindric" />
        <character name="shape" value="campanulate" />
        <character char_type="range_value" name="length" from="13" from_unit="mm" to="15" to_unit="mm" />
