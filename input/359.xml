  <flowering_time>fall</flowering_time>
  <habitat>stream banks</habitat>
  <habitat>mountain springs</habitat>
  <habitat>seeps</habitat>
  <habitat>cliffs near shore, moist shady places</habitat>
  <elevation>0–1500 m;</elevation>
  <ca_distribution>Alta.</ca_distribution>
  <ca_distribution>B.C.</ca_distribution>
