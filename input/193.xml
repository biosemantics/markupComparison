      <structure id="o2669" name="floret" constraint="peripheral">
        <character name="architecture" value="neuter" />
      </structure>
    </statement>
    <statement id="193.txt-13">
      <text>corollas pink to purple, [blue, or yellow] , spreading, lobes (5–6), linear;</text>
      <structure id="o2670" name="corolla">
        <character char_type="range_value" name="coloration" from="pink" to="purple" />
