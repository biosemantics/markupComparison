      <structure id="o13554" name="corolla" constraint="staminate">
        <character char_type="range_value" name="size" from="3" from_unit="mm" to="4" to_unit="mm" />
      </structure>
    </statement>
    <statement id="648.txt-12">
      <text>pistillate 3.5–6.5(–8) mm.</text>
      <structure id="o13555" name="corolla" constraint="pistillate">
        <character char_type="range_value" name="atypical_size" from="6.5" to="8" from_unit="mm" to_unit="mm" from_inclusive="false" />
        <character char_type="range_value" name="size" from="3.5" from_unit="mm" to="6.5" to_unit="mm" />
      </structure>
    </statement>
    <statement id="648.txt-13">
      <text>Cypselae 0.8–2 mm, ± papillate;</text>
