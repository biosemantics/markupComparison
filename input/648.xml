      <text>staminate 4–4.5 mm;</text>
      <structure id="o13558" name="pappus" constraint="staminate">
        <character char_type="range_value" name="size" from="4" from_unit="mm" to="4.5" to_unit="mm" />
      </structure>
    </statement>
    <statement id="648.txt-16">
      <text>pistillate 5.5–9 mm.</text>
      <structure id="o13559" name="pappus" constraint="pistillate">
        <character char_type="range_value" name="size" from="5.5" from_unit="mm" to="9" to_unit="mm" />
      </structure>
    </statement>
    <statement id="648.txt-17">
      <text>2 n = 56, 84, 140 (under A. neodioica ).</text>
      <structure id="o13560" name="chromosome">
