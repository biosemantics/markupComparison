        <character char_type="range_value" name="atypical_count" from="5" to="6" from_inclusive="false" />
        <character name="count" value="5" />
        <character name="coloration" value="green" modifier="pale" />
        <character name="coloration" value="blackish" modifier="often" constraint="at bases, apices" constraintid="o6802  o6803" />
        <character char_type="range_value" name="shape" from="linear" to="lanceolate" />
        <character name="shape" value="linear" />
        <character name="shape" value="lanceolate" />
        <character char_type="range_value" name="size" from="10" from_unit="mm" to="12" to_unit="mm" />
        <character name="pubescence" value="glabrous" />
        <character name="pubescence" value="hairy" modifier="sparsely" />
      </structure>
      <structure id="o6802" name="base" />
      <structure id="o6803" name="apex" />
