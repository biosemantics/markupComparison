  <common_name>Tall rattlesnakeroot, prenanthe élevée</common_name>
  <synonym_of_species_name>Nabalus altissimus (Linnaeus) Hooker; Prenanthes altissima var. cinnamomea Fernald; P. altissima var. hispidula Fernald</synonym_of_species_name>
  <description>
    <statement id="361.txt-0">
      <text>Plants 40–250 cm;</text>
      <structure id="o6783" name="whole_organism">
        <character char_type="range_value" name="size" from="40" from_unit="cm" to="250" to_unit="cm" />
        <character name="life_style" value="plant" />
      </structure>
    </statement>
    <statement id="361.txt-1">
      <text>taproots thickened, knotty, tuberous.</text>
      <structure id="o6784" name="taproot">
        <character name="thickness" value="thickened" />
        <character name="shape" value="knotty" />
        <character name="architecture" value="tuberous" />
      </structure>
    </statement>
    <statement id="361.txt-2">
      <text>Stems erect, greenish to purplish, glabrous proximally, often tomentulose distally.</text>
      <structure id="o6785" name="stem">
        <character name="orientation" value="erect" />
        <character char_type="range_value" name="coloration" from="greenish" to="purplish" />
        <character name="coloration" value="greenish" />
        <character name="coloration" value="purplish" />
        <character name="pubescence" value="glabrous" modifier="proximally" />
        <character name="pubescence" value="tomentulose" modifier="often ; distally" />
      </structure>
    </statement>
    <statement id="361.txt-3">
      <text>Leaves:</text>
      <structure id="o6786" name="leaf" />
    </statement>
    <statement id="361.txt-4">
      <text>proximal present at flowering;</text>
      <structure id="o6787" name="leaf" constraint="proximal">
        <character name="presence" value="present" modifier="at flowering" />
      </structure>
    </statement>
    <statement id="361.txt-5">
      <text>petioles winged;</text>
      <structure id="o6788" name="petiole">
        <character name="architecture" value="winged" />
      </structure>
    </statement>
    <statement id="361.txt-6">
      <text>blades usually ovate or triangular, 4–15 × 2–16 cm, thin, bases truncate to hastate or cordate, margins entire or shallowly dentate, often deeply 3-lobed, faces glabrous or with scattered hairs on veins;</text>
      <structure id="o6789" name="blade">
        <character name="shape" value="ovate" modifier="usually" />
        <character name="shape" value="triangular" modifier="usually" />
        <character char_type="range_value" name="length" from="4" from_unit="cm" to="15" to_unit="cm" />
        <character char_type="range_value" name="width" from="2" from_unit="cm" to="16" to_unit="cm" />
        <character name="thickness" value="thin" />
      </structure>
      <structure id="o6790" name="base">
        <character char_type="range_value" name="shape" from="truncate" to="hastate or cordate" />
        <character name="shape" value="truncate" />
        <character name="shape" value="hastate" />
        <character name="shape" value="cordate" />
      </structure>
      <structure id="o6791" name="margin">
        <character name="shape" value="entire" />
        <character name="shape" value="dentate" modifier="shallowly" />
        <character name="shape" value="3-lobed" modifier="often deeply" />
      </structure>
      <structure id="o6792" name="face">
        <character name="pubescence" value="glabrous" />
        <character name="pubescence" value="with scattered hairs" />
      </structure>
      <structure id="o6793" name="hair">
        <character name="arrangement" value="scattered" />
      </structure>
      <relation id="r842" name="with" from="o6792" to="o6793" negation="false" />
      <structure id="o6794" name="vein" />
      <relation id="r843" name="on" from="o6793" to="o6794" negation="false" />
    </statement>
    <statement id="361.txt-7">
      <text>distal reduced in size and lobing.</text>
      <structure id="o6795" name="leaf" constraint="distal">
        <character name="size" value="reduced" modifier="in size" />
        <character name="shape" value="lobing" />
      </structure>
    </statement>
    <statement id="361.txt-8">
      <text>Heads in narrow or spreading, paniculiform arrays.</text>
      <structure id="o6796" name="head" />
      <structure id="o6797" name="array">
        <character name="size" value="narrow" />
        <character name="orientation" value="spreading" />
        <character name="architecture" value="paniculiform" />
      </structure>
      <relation id="r844" name="in" from="o6796" to="o6797" negation="false" />
    </statement>
    <statement id="361.txt-9">
      <text>Involucres cylindric, 9–14 × 2–3 mm.</text>
      <structure id="o6798" name="involucre">
