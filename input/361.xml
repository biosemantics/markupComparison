    </statement>
    <statement id="361.txt-14">
      <text>Cypselae brown to tan, subcylindric, subterete, 4–5 mm, indistinctly 5–10-ribbed;</text>
      <structure id="o6806" name="cypsela">
        <character char_type="range_value" name="coloration" from="brown" to="tan" />
        <character name="coloration" value="brown" />
        <character name="coloration" value="tan" />
        <character name="shape" value="subcylindric" />
        <character name="shape" value="subterete" />
