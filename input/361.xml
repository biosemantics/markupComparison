        <character char_type="range_value" name="length" from="9" from_unit="mm" to="14" to_unit="mm" />
        <character char_type="range_value" name="width" from="2" from_unit="mm" to="3" to_unit="mm" />
      </structure>
    </statement>
    <statement id="361.txt-10">
      <text>Calyculi of 4–6, blackish, triangular bractlets 1–4 mm, glabrous.</text>
      <structure id="o6799" name="calyculus">
