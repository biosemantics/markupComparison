    <statement id="361.txt-13">
      <text>corollas pale yellow to greenish yellow, 7–15 mm.</text>
      <structure id="o6805" name="corolla">
        <character char_type="range_value" name="coloration" from="pale-yellow" to="greenish-yellow" />
